  "homepage": "https://bitbucket.org/bulletfight/r#readme",
  "devDependencies": {
    "copyfiles": "^1.2.0",
    "rimraf": "^2.6.2",
    "tslint": "~5.10.0",
    "typescript": "2.6.2",
    "wt-cli": "^9.4.0"
  },
  "dependencies": {
    "@types/nightmare": "2.10.4",
    "@types/request": "^2.47.0",
    "@types/request-promise": "^4.1.41",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
